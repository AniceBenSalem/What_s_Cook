DROP TABLE IF EXISTS `Recettes`;

CREATE TABLE `Recettes` (`NumRecette` INTEGER NOT NULL DEFAULT 0, `TitreRecette` VARCHAR(255), `NbPersonne` INTEGER DEFAULT 0, `NbPersonneTxt` VARCHAR(255), `Temps` VARCHAR(255), `Ingredients` LONGTEXT, `TxtRecette` LONGTEXT,  PRIMARY KEY (`NumRecette`));


